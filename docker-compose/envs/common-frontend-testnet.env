# =============================================================================
# DOS Chain Testnet - Frontend Environment OVERRIDES
# Base: common-frontend.env (mainnet)
# Only values that differ from base are listed here.
# Docker Compose loads base first, then this file overrides.
# =============================================================================

# App
NEXT_PUBLIC_APP_HOST=test.doscan.io

# Network
NEXT_PUBLIC_NETWORK_NAME=DOS Chain Testnet
NEXT_PUBLIC_NETWORK_ID=3939
NEXT_PUBLIC_NETWORK_RPC_URL=https://test.doschain.com
NEXT_PUBLIC_IS_TESTNET=true

# API
NEXT_PUBLIC_API_HOST=test-api.doscan.io

# Featured networks (testnet variant)
NEXT_PUBLIC_FEATURED_NETWORKS=https://raw.githubusercontent.com/DOSLabs/DOScan-Frontend-Configs/main/configs/featured-networks/dos-testnet.json

# SEO
NEXT_PUBLIC_OG_DESCRIPTION=Blockchain explorer for DOS Chain Testnet. DOScan is an analytics tool for exploring DOS transactions, addresses, statistics, and other platform activities.

# Explorer links
NEXT_PUBLIC_NETWORK_EXPLORERS=[{'title':'DOS Chain Testnet','baseUrl':'https://test.doscan.io/','paths':{'tx':'/tx','address':'/address','token':'/token','block':'/block'}}]

# Testnet-specific: hide native coin price
NEXT_PUBLIC_HIDE_NATIVE_COIN_PRICE=true

# User Ops - testnet has separate API host
NEXT_PUBLIC_USER_OPS_INDEXER_API_HOST=https://test-ops.doscan.io

# Stats & Visualize
NEXT_PUBLIC_STATS_API_HOST=https://test-stats.doscan.io
NEXT_PUBLIC_VISUALIZE_API_HOST=https://test-viz.doscan.io

# Metadata - disable on testnet (no service available)
NEXT_PUBLIC_METADATA_SERVICE_API_HOST=
NEXT_PUBLIC_METADATA_ADDRESS_TAGS_UPDATE_ENABLED=false

# Metasuites - disable on testnet
NEXT_PUBLIC_METASUITES_ENABLED=false
